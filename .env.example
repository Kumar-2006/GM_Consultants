# GM Consultants - Environment Variables
# Copy this file to .env and fill in real values

# MongoDB Atlas connection string (required)
# Format examples:
# mongodb+srv://USERNAME:PASSWORD@cluster0.xxxxx.mongodb.net/DATABASE_NAME?retryWrites=true&w=majority
# mongodb://USERNAME:PASSWORD@hostname1:27017,hostname2:27017/DATABASE_NAME?replicaSet=rs0&ssl=true
MONGO_URI=

# Session secret (required)
SESSION_SECRET=change-this-to-a-long-random-string

# Optional cookie settings
# COOKIE_DOMAIN=localhost
# SESSION_COOKIE_MAX_AGE_MS=604800000

# Runtime
NODE_ENV=development
PORT=5000MONGO_URI=your-mongodb-atlas-connection-string
JWT_SECRET=replace-with-secure-random-value
SESSION_SECRET=replace-with-secure-random-value
PORT=5000
NODE_ENV=development
CORS_ORIGINS=http://localhost:3000,https://your-vercel-app.vercel.app
COOKIE_DOMAIN=.your-domain.com
ALLOW_ADMIN_REGISTRATION=false
